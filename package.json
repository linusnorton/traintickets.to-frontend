{
  "name": "traintickets.to",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "immutability-helper": "^2.6.6",
    "jquery": "^3.3.1",
    "loaders.css": "^0.1.2",
    "material-ui": "^0.20.0",
    "moment": "^2.22.1",
    "querystring": "^0.2.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-easy-chart": "^0.1.16",
    "react-fontawesome": "^1.6.1",
    "react-google-charts": "^1.5.7",
    "react-google-maps": "^9.4.5",
    "react-loaders": "^3.0.1",
    "react-owl-carousel2": "^0.2.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "^1.1.4",
    "react-scrollarea": "^0.6.15",
    "react-token-autocomplete": "^0.5.3",
    "redux": "^3.7.1",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "deploy": "npm install && npm run build && rm ./dist/ -rf && mv ./build/ ./dist/",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2"
  }
