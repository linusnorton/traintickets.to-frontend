{
  "name": "traintickets.to",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-autosuggest": "^9.3.4",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.1",
    "react-google-charts": "^1.6.6",
    "typescript-debounce-decorator": "0.0.17"
  },
  "scripts": {
    "start-js": "react-scripts-ts start",
    "start": "REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.6.21",
    "@types/react": "^16.3.17",
    "@types/react-autosuggest": "^9.3.4",
    "@types/react-datepicker": "^1.1.3",
    "@types/react-dates": "^16.7.0",
    "@types/react-dom": "^16.0.6",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "react-scripts-ts": "^2.16.0",
    "typescript": "^2.9.2"
  }
}
