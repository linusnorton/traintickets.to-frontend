{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "immutability-helper": "^2.5.0",
    "jquery": "^3.2.1",
    "loaders.css": "^0.1.2",
    "material-ui": "^0.19.4",
    "moment": "^2.19.1",
    "querystring": "^0.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-google-maps": "^9.3.0",
    "react-loaders": "^3.0.1",
    "react-owl-carousel2": "^0.2.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.10",
    "react-scrollbar": "^0.5.1",
    "react-token-autocomplete": "^0.5.3",
